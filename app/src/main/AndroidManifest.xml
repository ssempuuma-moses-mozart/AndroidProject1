<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidproject">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:name=".AndroidProject"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <receiver
            android:name=".MyBroadCastReciever"
            android:enabled="true"
            android:exported="true"></receiver>

        <activity android:name=".UserRecycler" />
        <activity
            android:name=".MainActivity"
            android:label="Home Activity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".ListViewActivity" />
        <activity android:name=".Display" />
        <activity android:name=".Show" />
        <activity android:name=".Aspire" />
        <activity android:name=".Screen" />
        <activity android:name=".DisplayMessageActivity" />


        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>